<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="wrap_content">

    <View
        android:id="@+id/divider1V"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:background="@drawable/bg_inline_corner_grey"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintDimensionRatio="1.2:1"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.0" />

    <View
        android:id="@+id/divider1Text1V"
        android:layout_width="0dp"
        android:layout_height="3dp"
        android:layout_marginTop="16dp"
        android:layout_marginEnd="16dp"
        android:background="@color/colorGrey"
        app:layout_constraintBottom_toBottomOf="@+id/divider1Text2V"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toEndOf="@+id/divider1Text2V"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="1.0" />

    <View
        android:id="@+id/divider1Text2V"
        android:layout_width="3dp"
        android:layout_height="0dp"
        android:layout_marginStart="16dp"
        android:layout_marginTop="16dp"
        android:layout_marginEnd="16dp"
        android:layout_marginBottom="16dp"
        android:background="@color/colorGrey"
        app:layout_constraintBottom_toBottomOf="@+id/divider1V"
        app:layout_constraintEnd_toEndOf="@+id/divider1V"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/divider1V"
        app:layout_constraintTop_toBottomOf="@+id/divider1Text3V"
        app:layout_constraintVertical_bias="0.0" />

    <View
        android:id="@+id/divider1Text3V"
        android:layout_width="0dp"
        android:layout_height="16dp"
        android:layout_marginStart="64dp"
        android:layout_marginTop="16dp"
        android:layout_marginEnd="64dp"
        android:background="@drawable/bg_skeleton_grey"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toEndOf="@+id/divider1Text2V"
        app:layout_constraintTop_toTopOf="parent" />

    <include
        android:id="@+id/layout1"
        layout="@layout/content_chart_shimmer"
        android:layout_width="wrap_content"
        android:layout_height="0dp"
        app:layout_constraintBottom_toTopOf="@+id/divider1Text1V"
        app:layout_constraintStart_toEndOf="@+id/divider1Text2V"
        app:layout_constraintTop_toTopOf="@+id/divider1Text2V" />

    <include
        android:id="@+id/layout2"
        layout="@layout/content_chart_shimmer"
        android:layout_width="wrap_content"
        android:layout_height="0dp"
        app:layout_constraintBottom_toTopOf="@+id/divider1Text1V"
        app:layout_constraintStart_toEndOf="@+id/layout1"
        app:layout_constraintTop_toTopOf="@+id/divider1Text2V" />

    <include
        android:id="@+id/layout3"
        layout="@layout/content_chart_shimmer"
        android:layout_width="wrap_content"
        android:layout_height="0dp"
        app:layout_constraintBottom_toTopOf="@+id/divider1Text1V"
        app:layout_constraintStart_toEndOf="@+id/layout2"
        app:layout_constraintTop_toTopOf="@+id/divider1Text2V" />

    <include
        android:id="@+id/layout4"
        layout="@layout/content_chart_shimmer"
        android:layout_width="wrap_content"
        android:layout_height="0dp"
        app:layout_constraintBottom_toTopOf="@+id/divider1Text1V"
        app:layout_constraintStart_toEndOf="@+id/layout3"
        app:layout_constraintTop_toTopOf="@+id/divider1Text2V" />

    <include
        android:id="@+id/layout5"
        layout="@layout/content_chart_shimmer"
        android:layout_width="wrap_content"
        android:layout_height="0dp"
        app:layout_constraintBottom_toTopOf="@+id/divider1Text1V"
        app:layout_constraintStart_toEndOf="@+id/layout4"
        app:layout_constraintTop_toTopOf="@+id/divider1Text2V" />

    <include
        android:id="@+id/layout6"
        layout="@layout/content_chart_shimmer"
        android:layout_width="wrap_content"
        android:layout_height="0dp"
        app:layout_constraintBottom_toTopOf="@+id/divider1Text1V"
        app:layout_constraintEnd_toEndOf="@+id/divider1V"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toEndOf="@+id/layout5"
        app:layout_constraintTop_toTopOf="@+id/divider1Text2V" />

    <include
        android:id="@+id/layout7"
        layout="@layout/content_chart_shimmer"
        android:layout_width="wrap_content"
        android:layout_height="0dp"
        app:layout_constraintBottom_toTopOf="@+id/divider1Text1V"
        app:layout_constraintEnd_toEndOf="@+id/divider1V"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toEndOf="@+id/layout6"
        app:layout_constraintTop_toTopOf="@+id/divider1Text2V" />

</androidx.constraintlayout.widget.ConstraintLayout>